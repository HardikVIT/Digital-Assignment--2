#include<stdio.h>
#include <string.h>
struct office {
   char name[20];
   int age;
   char position[20];
   char date[50];
} e[10], temp;
 
void main() {
   int i, j, n;
   scanf("%d",&n);
   for (i = 0; i < 3; i++) {
      scanf("%s", e[i].name);
      scanf("%d",& e[i].age);
      scanf("%s", e[i].position);
      scanf("%s", e[i].date);
   }
   for (i = 0; i < n-1; i++){
        if (strcmp(e[i].name, e[i + 1].name) > 0) {
            temp = e[i];
            e[i] = e[i + 1];
            e[i + 1] = temp;
        }
            
    }
 
   for (i = 0; i < n; i++) {
      printf("%s\n",e[i].name);
      printf("%d\n",e[i].age);
      printf("%s\n",e[i].position);
      printf("%s\n",e[i].date);
   }
   for (i = 0; i < n-1; i++){
        if (strcmp(e[i].date, e[i + 1].date) > 0) {
            temp = e[i];
            e[i] = e[i + 1];
            e[i + 1] = temp;
        }
            
    }
 
   for (i = 0; i < n; i++) {
      printf("%s\n",e[i].name);
      printf("%d\n",e[i].age);
      printf("%s\n",e[i].position);
      printf("%s\n",e[i].date);
   }
}
